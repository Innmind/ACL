{
    "name": "innmind/acl",
    "type": "library",
    "description": "Reproduce the filesystem ACL mechanism",
    "keywords": ["acl"],
    "homepage": "http://github.com/Innmind/ACL",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/ACL/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/immutable": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\ACL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\ACL\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "^4.22",
        "nikic/php-parser": "^4.13.2",
        "innmind/black-box": "^4.16",
        "innmind/coding-standard": "~2.0"
    }
}
